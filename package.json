{
  "homepage": "http://ZeLars3.github.io/storybook-toast",
  "name": "toast",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.4.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "path": "^0.12.7",
